{
  "name": "panorama",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"pnpm vite:dev\" \"pnpm wg\"",
    "debug": "concurrently \"pnpm vite:dev\" \"pnpm wg:debug\"",
    "check": "tsc --noEmit",
    "codegen": "wunderctl generate",
    "clean": "rimraf dist **/generated*",
    "reinstall": "pnpm clean && rimraf node_modules && pnpm i",
    "wg": "wunderctl up",
    "wg:debug": "wunderctl up --debug",
    "wg:trace": "wunderctl up -l trace",
    "vite:dev": "vite --port 5000 --mode development",
    "vite:build": "vite build",
    "build": "pnpm clean && pnpm codegen && pnpm test && npm run vite:build",
    "preview": "vite preview",
    "lint": "pnpm codegen && eslint src wundergraph",
    "lint:fix": "pnpm codegen && eslint --fix src wundergraph",
    "test": "pnpm wunderctl generate --env .env.test && vitest run"
  },
  "dependencies": {
    "@wundergraph/sdk": "^0.184.0",
    "@wundergraph/swr": "^0.19.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^6.0.0",
    "docker-compose": "^1.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "graphql-type-json": "^0.3.2",
    "jsdom": "^25.0.1",
    "node-fetch": "^2.6.7",
    "open-cli": "^7.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitest": "^2.1.8",
    "wait-on": "^6.0.0"
  }
}